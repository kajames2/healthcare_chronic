        "jmin": 0,
	      "alpha": 0.125,
		"beta": 0.2,
	      "x_0": 60,
        "j": 20,
        "max_effect": 1.8,
        "k": 100
    },
    "probability": {
	      "function_name" : "Base",
        "function" : "((exp(age_rate * (s + age))-.8 + exp(shock_rate * shocks)) - 1) * (1 - max_effect * (fitness / (fitness + j)))",
        "age_rate": 0.0025,
        "shock_rate": 0.066,
        "max_effect": 1.8,
        "j": 100,
	      "s": 0
    },
    "fitness": {
	      "function_name" : "AgeLinear",
         "function" : "(1-p-r*age) * fitness + x / (price * (.25 + age/53.33))",
