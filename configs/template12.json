{
    "max_age": 40,
    "decision_periods": 40,
    "max_fitness": 100,
    "min_debt_payment": 1,
    "shock_count_size": 1,
    "initial_fitness": 50,
    "fair_draws": true,
    "n_samples": 10000,
    "job": {
	      "function_name" : "Base",
